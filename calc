#include <iostream>
using namespace std;

int main ()
{
	char operation;
	float num1, num2;
	
	cout << "Enter operator (+,-,*,/):";
	cin >> operation; 
	
	cout << "Enter two numbers:";
	cin >> num1 >> num2;
	
	switch (operation) {
		case '+': 
		cout << "Result: " << num1 + num2;
		break;
		
		case '-':
			cout << "Result: " << num1 - num2;
			break;
			
		case '*': 
		cout << "Result: " << num1*num2;
		break;
		
		case '/':
			cout << "Result: " << num1/num2;
			break;
		
	}
	return 0;
	
}
